<body>
sdf
<div style="height:700px;  width: 400px" name="cy" id="cy"></div>
<script>
$('#cy').cytoscape({
  style: cytoscape.stylesheet()
    .selector('node')
      .css({
        'content': 'data(name)',
        'text-valign': 'center',
        'color': 'white',
        'text-outline-width': 2,
        'text-outline-color': '#888',
      })
    .selector('edge')
      .css({
        'target-arrow-shape': 'triangle'
      })
    .selector(':selected')
      .css({
        'background-color': 'black',
        'line-color': 'black',
        'target-arrow-color': 'black',
        'source-arrow-color': 'black'
      })
    .selector('.faded')
      .css({
        'opacity': 0.25,
        'text-opacity': 0
      }),

  elements: {
    nodes: [
{ data: { id: '0', name: '0' } },{ data: { id: '1', name: '1' } },{ data: { id: '2', name: '2' } },{ data: { id: '3', name: '3' } },{ data: { id: '4', name: '4' } }
    ],
    edges: [
{ data: { source: '0', target: '1' } }{ data: { source: '0', target: '2' } }{ data: { source: '0', target: '3' } }{ data: { source: '0', target: '4' } }{ data: { source: '1', target: '0' } },{ data: { source: '1', target: '2' } },{ data: { source: '1', target: '3' } },{ data: { source: '1', target: '4' } }{ data: { source: '2', target: '0' } },{ data: { source: '2', target: '1' } },{ data: { source: '2', target: '3' } },{ data: { source: '2', target: '4' } }{ data: { source: '3', target: '0' } },{ data: { source: '3', target: '1' } },{ data: { source: '3', target: '2' } },{ data: { source: '3', target: '4' } }{ data: { source: '4', target: '0' } },{ data: { source: '4', target: '1' } },{ data: { source: '4', target: '2' } },{ data: { source: '4', target: '3' } }

    ]
  },

  ready: function(){
    window.cy = this;

    // giddy up...

    cy.elements().unselectify();

    cy.on('tap', 'node', function(e){
      var node = e.cyTarget;
      var neighborhood = node.neighborhood().add(node);

      cy.elements().addClass('faded');
      neighborhood.removeClass('faded');
    });

    cy.on('tap', function(e){
      if( e.cyTarget === cy ){
        cy.elements().removeClass('faded');
      }
    });
  }
});
</script>
</body>
</html>
